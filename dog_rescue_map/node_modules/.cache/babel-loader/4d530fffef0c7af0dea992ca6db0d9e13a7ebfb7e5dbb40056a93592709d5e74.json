           const res = await fetch(`https://api.petfinder.com/v2/animals?type=dog&location=${this.zip}&output=basic&format=json&limit=75`, {\r\n                    headers: {\r\n                        Authorization: `Bearer ${access_token}`,\r\n                    },\r\n                });\r\n                const data = await res.json();\r\n                console.log(data);\r\n\r\n                this.dogs = data.animals\r\n                \r\n                .filter(animal => animal.photos && animal.photos.length > 0)\r\n                .filter(animal => !animal.name.includes(\"HOLD\"))\r\n                \r\n                .map(animal => ({\r\n                    id: animal.id,\r\n                    name: animal.name.split(' ')[0],\r\n\r\n                    breed: animal.breeds.primary,\r\n                    age: animal.age,\r\n                    description: animal.description,\r\n                    photo: animal.photos && animal.photos.length > 0 ? animal.photos[0].small : null,\r\n                    distance: Math.round(animal.distance),\r\n                    url: animal.url,\r\n\r\n                }))\r\n\r\n                this.dogs.sort((a, b) => a.distance - b.distance);\r\n\r\n            }catch (error) {\r\n                console.error('Error: Could not access database', error);\r\n            }\r\n              \r\n        },\r\n    },\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n\r\n#home-header {\r\n    font-size: 3rem;\r\n    text-shadow: 2px 2px 5px black;\r\n}\r\n\r\n#text-box {\r\n    height: 30px;\r\n    width: 225px;\r\n    border-radius: 3px;\r\n    border: 2px solid black;\r\n    font-size: 1rem;\r\n    font-weight: 500;\r\n    margin-right: 15px;\r\n}\r\n\r\n#submit {\r\n    background-color: #d61c1c;\r\n    height: 35px;\r\n    font-size: 1.2rem;\r\n    border-radius: 3px;\r\n    font-weight: 500;\r\n    font-family: \"League Spartan\", sans-serif;\r\n    color: white;\r\n    padding-left: 10px;\r\n    padding-right: 10px;\r\n    transition: transform 0.2s ease;\r\n}\r\n\r\n#submit:hover {\r\n    cursor: pointer;  \r\n}\r\n\r\n#submit:active {\r\n    transition: transform 0.2s ease;\r\n}\r\n\r\n\r\n#search-form {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-top: 150px;\r\n    margin-right: 300px;\r\n}\r\n\r\n</style>\r\n\r\n"],"mappings":"AAmBA,OAAOA,OAAM,MAAO,0BAA0B;AAG1C,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,GAAG,EAAE,EAAE;MACPC,IAAI,EAAE;IACV;EACJ,CAAC;EAEDC,UAAU,EAAE;IACRJ;EACJ,CAAC;EAEDK,OAAO,EAAE;IACN,MAAMC,MAAMA,CAAA,EAAG;MAEd,IAAI;QACA,MAAMC,aAAY,GAAI,MAAMC,KAAK,CAAC,2CAA2C,EAAE;UAC3EC,MAAM,EAAE,MAAM;UACdC,OAAO,EAAE;YACL,cAAc,EAAE;UACpB,CAAC;UACDC,IAAI,EAAEC,IAAI,CAACC,SAAS,CAAC;YACjBC,UAAU,EAAE,oBAAoB;YAChCC,SAAS,EAAE,oDAAoD;YAC/DC,aAAa,EAAE;UAEnB,CAAC;QACL,CAAC,CAAC;QACF,MAAM;UAAEC;QAAa,IAAI,MAAMV,aAAa,CAACW,IAAI,CAAC,CAAC;QAGnD,MAAMC,GAAE,GAAI,MAAMX,KAAK,CAAE,0DAAyD,IAAI,CAACN,GAAI,oCAAmC,EAAE;UAC5HQ,OAAO,EAAE;YACLU,aAAa,EAAG,UAASH,YAAa;UAC1C;QACJ,CAAC,CAAC;QACF,MAAMhB,IAAG,GAAI,MAAMkB,GAAG,CAACD,IAAI,CAAC,CAAC;QAC7BG,OAAO,CAACC,GAAG,CAACrB,IAAI,CAAC;QAEjB,IAAI,CAACE,IAAG,GAAIF,IAAI,CAACsB,OAAM,CAEtBC,MAAM,CAACC,MAAK,IAAKA,MAAM,CAACC,MAAK,IAAKD,MAAM,CAACC,MAAM,CAACC,MAAK,GAAI,CAAC,EAC1DH,MAAM,CAACC,MAAK,IAAK,CAACA,MAAM,CAACG,IAAI,CAACC,QAAQ,CAAC,MAAM,CAAC,EAE9CC,GAAG,CAACL,MAAK,KAAM;UACZM,EAAE,EAAEN,MAAM,CAACM,EAAE;UACbH,IAAI,EAAEH,MAAM,CAACG,IAAI,CAACI,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;UAE/BC,KAAK,EAAER,MAAM,CAACS,MAAM,CAACC,OAAO;UAC5BC,GAAG,EAAEX,MAAM,CAACW,GAAG;UACfC,WAAW,EAAEZ,MAAM,CAACY,WAAW;UAC/BC,KAAK,EAAEb,MAAM,CAACC,MAAK,IAAKD,MAAM,CAACC,MAAM,CAACC,MAAK,GAAI,IAAIF,MAAM,CAACC,MAAM,CAAC,CAAC,CAAC,CAACa,KAAI,GAAI,IAAI;UAChFC,QAAQ,EAAEC,IAAI,CAACC,KAAK,CAACjB,MAAM,CAACe,QAAQ,CAAC;UACrCG,GAAG,EAAElB,MAAM,CAACkB;QAEhB,CAAC,CAAC;QAEF,IAAI,CAACxC,IAAI,CAACyC,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAKD,CAAC,CAACL,QAAO,GAAIM,CAAC,CAACN,QAAQ,CAAC;MAErD,CAAC,QAAOO,KAAK,EAAE;QACX1B,OAAO,CAAC0B,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC;MAC5D;IAEJ;EAC